{
  "name": "braindrive-chat-with-your-documents",
  "version": "1.0.0",
  "description": "BrainDrive Plugin Template - A minimal starter template for creating BrainDrive plugins",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "start": "webpack serve --mode development",
    "clean": "rm -rf dist",
    "pack:zip": "npm run build && cd dist && zip -r ../artifact.zip .",
    "pack:tar": "npm run build && cd dist && tar -czf ../artifact.tar.gz .",
    "test": "ts-node --transpile-only --require ./test_setup.js test_component.js",
    "test-dom": "ts-node --transpile-only --require ./tests/test_dom_setup.js ./tests/test_component.js"
  },
  "keywords": [
    "braindrive",
    "plugin",
    "template",
    "react",
    "typescript",
    "starter"
  ],
  "author": "BrainDrive",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^24.0.15",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "jsdom": "^26.1.0",
    "semantic-release": "^24.2.7",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.11"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
